/* 水平间距 */
/* 水平间距 */
.bottom {
  display: flex;
  flex-wrap: wrap;
  margin-top: 10px;
}
.bottom text {
  margin-left: 10px;
}
.top {
  background-color: #c00000;
}
.middle {
  display: flex;
  justify-content: space-between;
  padding: 0 10px;
  border-bottom: 1px solid gray;
  line-height: 40px;
}
.list {
  display: flex;
  line-height: 40px;
  justify-content: space-between;
  padding: 0 10px;
  border-bottom: 1px solid gray;
}
.list text {
  white-space: nowrap;
  overflow: hidden;
  text-overflow: ellipsis;
}

